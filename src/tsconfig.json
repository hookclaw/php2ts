{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es5",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "inlineSourceMap": false,
        "outDir": ".."
    },
    "exclude": [
        "typings"
    ]
}